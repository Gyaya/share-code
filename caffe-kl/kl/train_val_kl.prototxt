  }
}
layer {
  name: "my_fc8"
  bottom: "fc7"
  top: "my_fc8"
  type: "InnerProduct"
  inner_product_param {
    num_output: 8      #change
  }
  param {
    lr_mult: 10
  }
  param {
    lr_mult: 20
  }
}
# new softmax loss
layer {
  name: "loss/softmax"
  type: "SoftmaxWithKLLoss"
  bottom: "my_fc8"
  bottom: "label" #label distribution, use the largest as single label
  top: "loss/softmax"
  loss_weight: 0.8
}

# kl loss
layer {
  name: "loss/kl"
  type: "SoftmaxWithMyKLLoss"
  bottom: "my_fc8"
  bottom: "label" #label distribution
  top: "loss/kl"
  loss_weight: 0.2
}

# new Accuracy, use the largest one in distribution as the predict label
layer {
  name: "accuracy/top1"
  type: "Accuracy"
  bottom: "my_fc8"
  bottom: "label"
  top: "accuracy@1"
  include: { phase: TEST }
  accuracy_param {
    top_k: 1
  }
}