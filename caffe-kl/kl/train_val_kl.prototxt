name: "VGG_ILSVRC_16_layers"
layer {
  name: "data"
  type: "Data"
  include {
    phase: TRAIN
  }
 transform_param {
    crop_size: 224
    mean_file: "/home/shedongyu/sdy/train/data/lmdb/emotion_mean.binaryproto"
    #mean_value: 104
    #mean_value: 117
    #mean_value: 123
    mirror: true
 }
 data_param {
    source: "/home/shedongyu/sdy/train/data/lmdb/emotion_train_lmdb"
